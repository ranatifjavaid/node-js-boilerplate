{
  "name": "kwik-trust",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "cross-env NODE_ENV=test mocha --timeout 10000 ./test/**/* --exit --",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "migrate:up": "node scripts/migrate-up",
    "migrate:undo": "node scripts/migrate-undo",
    "migrate:undo:all": "node scripts/migrate-undo-all",
    "seed:up": "node scripts/seed-up",
    "seed:all": "node scripts/seed-all",
    "seed:undo": "node scripts/seed-undo"
  },
  "dependencies": {
    "aws-sdk": "^2.1179.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "buffer": "^6.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.17.0",
    "ethers": "^5.6.9",
    "express": "~4.16.1",
    "express-fileupload": "^1.4.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.5.2",
    "ipfs-http-client": "^56.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemailer": "^6.7.5",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "sequelize-cli": "^6.4.1"
  }
}
